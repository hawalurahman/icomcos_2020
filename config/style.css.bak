a {
    color : #06D;
    text-decoration:none;
}

a:hover {
    text-decoration:underline;

}

.moseover{
position:relative;
z-index:0;
}
.moseover:hover{
z-index:150;
}

.moseover span{
	position:absolute;
	padding:0px;
	left:0px;
	visibility:hidden;
}

.moseover span img{
	border-width:0;
	padding:0px;
}

.moseover:hover img{
	visibility:visible;
	top:0px;
	left:0px;
}

body{
	font-family:Georgia;/*Tahoma;*/
}

p{
	margin-bottom:10px;
	margin-left:2px;
	margin-right:2px;
	padding-left:5px;
	padding-right:5px;
/*	font-family:Georgia;*/
	font-family:Tahoma;
/*	font-size:small;*/
	font-color:#330033;
	text-align:justify;
	line-height:0.7cm;
}

.kiri{
	border-color:blue;
	font-size:small;
	font-color:#330033;
	text-align :left;
}

.isi{
	padding-left : 5px;
	margin : 0px;
	font-size : 10pt;
        font-family:Tahoma;
}

.judulmenu{
	border-top : 1px solid #add8e6;
	border-left : 1px solid #add8e6;
	border-right : 1px solid #add8e6;
	padding-top : 2px;
	padding-bottom : 2px;
	margin : 0px;
	text-align : center;
	font-size : 16pt;
	font-weight: bold;
	color:#FFFFFF;
	background-color : #000080;/*#0000A0;/*#6495ed;*/
	width : 180px;
}

.menu{
	border : 1px solid #0000A0;/*#add8e6;*/
	padding : 0px;
	margin : 0px;
	text-align : middle;
	font-size : 12pt;
	width : 180px;
}

.menu a{
	display : block;
	border-top : 1px;
	border-left : 1px;
	border-right : 1px;
	border-bottom : 1px;
	font-family:arial;
	text-align : center;
	color : #FFCC00;/*#EDEDED;*/
	text-decoration : none;
}

.menu a:link{
	color : #4C86C0;/*#EDEDED;*/
}

.menu a:visited{
	color : #0000A0;/*#4C86C0;*/
}

.menu a:hover{
	display : block;
	text-align : center;
	font-size : 12pt;
	font-weight: bold;
	background-color : #000080;/*#336699;*/
	color : #FFCC00;/*#0000A0;*/
	text-decoration : none;
}

fieldset{
border:1px solid #781351;
width:96%;
}

legend{
background:#FFFFFF;
padding: 2px 6px;
